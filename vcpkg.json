{
  "name": "spore-codegen",
  "version": "1.0",
  "builtin-baseline": "74e6536215718009aae747d86d84b78376bf9e09",
  "dependencies": [
    "argparse",
    "catch2",
    "inja",
    "nlohmann-json",
    "picosha2",
    "spdlog",
    "yaml-cpp"
  ],
  "default-features": [
    "cpp",
    "spirv",
    "slang"
  ],
  "features": {
    "cpp": {
      "description": "support for clang-based C++ parser"
    },
    "spirv": {
      "description": "support for SPIR-V parser",
      "dependencies": [
        "spirv-reflect"
      ]
    },
    "slang": {
      "description": "support for Slang parser",
      "dependencies": [
        "shader-slang"
      ]
    }
  }
}
