{
  "name": "spore-codegen",
  "version": "1.0",
  "builtin-baseline": "b02e341c927f16d991edbd915d8ea43eac52096c",
  "dependencies": [
    "argparse",
    "catch2",
    "inja",
    "nlohmann-json",
    "picosha2",
    "spdlog",
    "yaml-cpp"
  ],
  "default-features": [
    "cpp",
    "spirv",
    "scripting-lua"
  ],
  "features": {
    "cpp": {
      "description": "Support for clang-based C++ parser"
    },
    "spirv": {
      "description": "Support for SPIR-V parser",
      "dependencies": [
        "spirv-reflect"
      ]
    },
    "scripting-lua": {
      "description": "Support for renderer scripting with LUA",
      "dependencies": [
        "lua",
        "sol2"
      ]
    }
  }
}
