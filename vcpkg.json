{
  "name": "spore-codegen",
  "version": "1.0",
  "builtin-baseline": "3508985146f1b1d248c67ead13f8f54be5b4f5da",
  "dependencies": [
    "argparse",
    "catch2",
    "fmt",
    "inja",
    "nlohmann-json",
    "picosha2",
    "spdlog",
    "tiny-process-library",
    "yaml-cpp"
  ],
  "default-features": [
    "cpp",
    "spirv",
    "scripting-lua"
  ],
  "features": {
    "cpp": {
      "description": "Support for Clang-based C++ parser"
    },
    "spirv": {
      "description": "Support for SPIR-V parser",
      "dependencies": [
        "spirv-reflect"
      ]
    },
    "scripting-lua": {
      "description": "Support for renderer scripting with LUA",
      "dependencies": [
        "lua"
      ]
    }
  }
}
